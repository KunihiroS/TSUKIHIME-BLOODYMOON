{
    "name": "Tsukihime Bloody Moon",
    "type": "dark",
    "colors": {
        "editor.background": "#4A0F20",
        "editor.foreground": "#C33150",
        "editorCursor.foreground": "#C33150",
        "editor.lineHighlightBackground": "#72163D",
        "editor.selectionBackground": "#72163D",
        "editor.inactiveSelectionBackground": "#72163D",
        "editorIndentGuide.background": "#72163D",
        "editorIndentGuide.activeBackground": "#C33150",
        "activityBar.background": "#72163D",
        "activityBar.foreground": "#C33150",
        "activityBarBadge.background": "#4A0F20",
        "activityBarBadge.foreground": "#ffffff",
        "sideBar.background": "#72163D",
        "sideBar.foreground": "#C33150",
        "sideBarTitle.foreground": "#C33150",
        "sideBarSectionHeader.background": "#4A0F20",
        "sideBarSectionHeader.foreground": "#C33150",
        "titleBar.activeBackground": "#72163D",
        "titleBar.activeForeground": "#C33150",
        "statusBar.background": "#72163D",
        "statusBar.foreground": "#C33150",
        "statusBar.noFolderBackground": "#72163D",
        "tab.activeBackground": "#4A0F20",
        "tab.inactiveBackground": "#72163D",
        "tab.activeForeground": "#C33150",
        "tab.inactiveForeground": "#C33150",
        "list.activeSelectionBackground": "#72163D",
        "list.activeSelectionForeground": "#ffffff",
        "list.hoverBackground": "#4A0F20",
        "list.hoverForeground": "#C33150",
        "list.inactiveSelectionBackground": "#72163D",
        "input.background": "#4A0F20",
        "input.foreground": "#C33150",
        "input.border": "#72163D",
        "button.background": "#72163D",
        "button.foreground": "#ffffff",
        "button.hoverBackground": "#C33150",
        "panel.background": "#72163D",
        "panel.border": "#4A0F20",
        "panelTitle.activeForeground": "#C33150",
        "panelTitle.inactiveForeground": "#C33150",
        "badge.background": "#4A0F20",
        "badge.foreground": "#ffffff",
        "scrollbarSlider.background": "#72163D",
        "scrollbarSlider.hoverBackground": "#72163D",
        "scrollbarSlider.activeBackground": "#72163D",
        "editor.selectionHighlightBackground": "#C33150",
        "selection.background": "#C33150"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#72163D",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#C33150"
            }
        },
        {
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#C33150"
            }
        },
        {
            "scope": ["entity.name.function", "support.function"],
            "settings": {
                "foreground": "#72163D"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#4A0F20"
            }
        }
    ]
}
