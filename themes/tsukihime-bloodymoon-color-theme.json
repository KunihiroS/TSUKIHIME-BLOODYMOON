{
    "name": "Tsukihime Bloody Moon",
    "type": "dark",
    "colors": {
        "editor.background": "#4A0F20",
        "editor.foreground": "#ff838b", // 常用フォントを淡いピンクに変更
        "editorCursor.foreground": "#FF5A7E",
        "editor.lineHighlightBackground": "#3A0A1F",
        "editor.selectionBackground": "#5A1030",
        "editor.inactiveSelectionBackground": "#3A0A1F",
        "editorIndentGuide.background": "#3A0A1F",
        "editorIndentGuide.activeBackground": "#790820",
        "activityBar.background": "#2C0812",
        "activityBar.foreground": "#FF5A7E",
        "activityBarBadge.background": "#790820",
        "activityBarBadge.foreground": "#ffffff",
        "sideBar.background": "#2C0812",
        "sideBar.foreground": "#ff838b", // サイドバーのフォントも同じ淡いピンクに
        "sideBarTitle.foreground": "#FF5A7E",
        "sideBarSectionHeader.background": "#3A0A1F",
        "sideBarSectionHeader.foreground": "#FF5A7E",
        "titleBar.activeBackground": "#2C0812",
        "titleBar.activeForeground": "#ff838b", // タイトルバーのフォントも同じ淡いピンクに
        "statusBar.background": "#2C0812",
        "statusBar.foreground": "#ff838b", // ステータスバーのフォントも同じ淡いピンクに
        "statusBar.noFolderBackground": "#2C0812",
        "tab.activeBackground": "#4A0F20",
        "tab.inactiveBackground": "#2C0812",
        "tab.activeForeground": "#ff838b", // タブのアクティブフォントも同じ淡いピンクに
        "tab.inactiveForeground": "#A08088",
        "list.activeSelectionBackground": "#5A1030",
        "list.activeSelectionForeground": "#ffffff",
        "list.hoverBackground": "#3A0A1F",
        "list.hoverForeground": "#ff838b", // リストのホバー時のフォントも同じ淡いピンクに
        "list.inactiveSelectionBackground": "#3A0A1F",
        "input.background": "#2C0812",
        "input.foreground": "#ff838b", // 入力欄のフォントも同じ淡いピンクに
        "input.border": "#5A1030",
        "button.background": "#5A1030",
        "button.foreground": "#ffffff",
        "button.hoverBackground": "#790820",
        "panel.background": "#2C0812",
        "panel.border": "#3A0A1F",
        "panelTitle.activeForeground": "#ff838b", // パネルタイトルのアクティブフォントも同じ淡いピンクに
        "panelTitle.inactiveForeground": "#A08088",
        "badge.background": "#790820",
        "badge.foreground": "#ffffff",
        "scrollbarSlider.background": "#5A1030AA",
        "scrollbarSlider.hoverBackground": "#72163DCC",
        "scrollbarSlider.activeBackground": "#790820AA",
        "editor.selectionHighlightBackground": "#5A1030",
        "selection.background": "#5A1030"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#A08088",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#FF7E9D"
            }
        },
        {
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#FF5A7E"
            }
        },
        {
            "scope": ["entity.name.function", "support.function"],
            "settings": {
                "foreground": "#FF9CAF"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#ff838b" // 変数のフォント色も同じ淡いピンクに
            }
        },
        {
            "scope": ["constant", "entity.name.type", "support.type"],
            "settings": {
                "foreground": "#FFB3C6"
            }
        },
        {
            "scope": ["punctuation", "meta.brace"],
            "settings": {
                "foreground": "#CCA2AD"
            }
        },
        {
            "scope": ["entity.name.tag", "support.class"],
            "settings": {
                "foreground": "#FF5A7E",
                "fontStyle": "bold"
            }
        }
    ]
}
